import React from 'react';
import './App.css';
import Topbar from './components/Topbar';
import Navbar from './components/Navbar';
 
function App() {
  return (
    <div className="App">
      <Topbar />
      <hr className="thin-line" />
      <Navbar />
      <div className="main-content">
        <h2>Dine & Code</h2>
        <h1>Debug your hunger here</h1>
        <button className="menu-btn">View Our Menu</button>
      </div>
            {/* Second Section - Scroll Down */}
            <div className="second-section">      
        <h1 className="section-title">Nothing Brings People Together Like Good Food</h1>
 
        <div className="menu-gallery">
          <div className="menu-item">
            <img src="breakfast.jpg" alt="Breakfast" />
            <h2>Breakfast</h2>
            <p>Start your day with a delightful breakfast.</p>
          </div>
 
          <div className="menu-item">
            <img src="cafe.jpg" alt="Cafe" />
            <h2>Cafe</h2>
            <p>Enjoy our signature cafe brews and bites.</p>
          </div>
 
          <div className="menu-item">
            <img src="meals.jpg" alt="Meals" />
            <h2>Meals</h2>
            <p>Hearty meals to satisfy your hunger.</p>
          </div>
        </div>
 
        {/* View Menu Button */}
        <div className="view-menu-btn">
