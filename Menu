import React from "react";
import { useNavigate, Link } from "react-router-dom"; // Import Link
import Dish from "./Dish";
import "./../styles/Menu.css";


// src/components/Dish.js

import React, { useState } from "react";
import "./../styles/Dish.css";

const Dish = ({ name, price, description, image }) => {
  const [quantity, setQuantity] = useState(0);

  const increment = () => {
    setQuantity(prev => prev + 1);
  };

  const decrement = () => {
    if (quantity > 0) {
      setQuantity(prev => prev - 1);
    }
  };

  const addToCart = () => {
    if (quantity > 0) {
      alert(`${quantity} ${name}(s) added to cart.`);
      // Here you can integrate with your cart logic
    } else {
      alert("Please select a quantity to add to cart.");
    }
  };

  return (
    <div className="dish">
      <img src={image} alt={name} className="dish-image" />
      <div className="dish-details">
        <h3>{name}</h3>
        <p>{description}</p>
        <p><strong>{price}</strong></p>

        {/* Quantity Controls */}
        <div className="quantity-controls">
          <button onClick={decrement} disabled={quantity === 0}>-</button>
          <span>{quantity}</span>
          <button onClick={increment}>+</button>
        </div>

        {/* Add to Cart Button */}
        <button 
          className="add-to-cart-button" 
          onClick={addToCart}
          disabled={quantity === 0}
        >
          Add to Cart
        </button>
      </div>
    </div>
  );
};

export default Dish;



/* src/styles/Dish.css */

.dish {
  border: 1px solid #ccc;
  padding: 10px;
  margin: 10px;
  border-radius: 8px;
  background: rgba(255, 255, 255, 0.9);
}

.dish-image {
  width: 100%;
  height: auto;
  border-radius: 8px;
}

.dish-details {
  margin-top: 10px;
}

.quantity-controls {
  display: flex;
  align-items: center;
  margin: 10px 0;
